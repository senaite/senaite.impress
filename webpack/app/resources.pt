<!-- senaite.impress: print CSS files (get also hooked into WeasyPrint when generating the PDF) -->
<link href="#" rel="stylesheet" tal:attributes="href string:${view/site_url}/++plone++senaite.impress.static/css/bootstrap.min.css" />
<link href="#" rel="stylesheet" tal:attributes="href string:${view/site_url}/++plone++senaite.impress.static/css/bootstrap-print.css" />
<link href="#" rel="stylesheet" tal:attributes="href string:${view/site_url}/++plone++senaite.impress.static/css/print.css" />

<!-- senaite.impress: main js/css -->
<% for (let js in htmlWebpackPlugin.files.js) { %>
<script tal:attributes="src <%= 'string:${view/site_url}/' + htmlWebpackPlugin.files.js[js] %>"></script>
<% } %>
<% for (let css in htmlWebpackPlugin.files.css) { %>
<link href="#" rel="stylesheet" tal:attributes="href <%= 'string:${view/site_url}/' + htmlWebpackPlugin.files.css[css] %>" />
<% } %>
